hpernit_1.f:C     + + + COMMON BLOCKS- SCRTCH, VERSION NITR2 + + +
hpernit_1.f:      IF (PONFP.GE.1)   PAD(PONFP +IVL1)= PONITR
hpernit_1.f:      SUBROUTINE   NITRPT
hpernit_1.f:C     + + + COMMON BLOCKS- SCRTCH, VERSION NITR2 + + +
hpernit_1.f:      SUBROUTINE   NITRST
hpernit_1.f:C     + + + COMMON BLOCKS- SCRTCH, VERSION NITR2 + + +
hpernit_1.f:      SUBROUTINE   NITRXN
hpernit_1.f:     M                    NDEFC,NWCNT,NECNT,AGPLTN,NIT,NITRXF  ! )
hpernit_1.f:     $            KRET,KRETF,ANUTF,SMST,NDEFC,AGPLTN,NIT(8),NITRXF(16)
hpernit_1.f:C     NITRXF - current reaction fluxes for this layer
hpernit_1.f:     $            AMMIF,IMMAM,UTAM,IMMNI,UTNI,NITRF,DENI,AMVO,RFON,
hpernit_1.f:      ADSAM= NITRXF(1)
hpernit_1.f:      DESAM= NITRXF(2)
hpernit_1.f:      AMMIF= NITRXF(3)
hpernit_1.f:      IMMAM= NITRXF(4)
hpernit_1.f:      UTAM = NITRXF(5)
hpernit_1.f:      IMMNI= NITRXF(6)
hpernit_1.f:      UTNI = NITRXF(7)
hpernit_1.f:      NITRF= NITRXF(8)
hpernit_1.f:      DENI = NITRXF(9)
hpernit_1.f:      AMVO = NITRXF(10)
hpernit_1.f:      RFON = NITRXF(11)
hpernit_1.f:      PRETL= NITRXF(12)
hpernit_1.f:      PRETR= NITRXF(13)
hpernit_1.f:      UTAMA= NITRXF(14)
hpernit_1.f:      UTNIA= NITRXF(15)
hpernit_1.f:      NFIX = NITRXF(16)
hpernit_1.f:          NITRF= AMSU*KNIK
hpernit_1.f:          NITRF= 0.0
hpernit_1.f:      TNO3= NO3+ NITRF- (IMMNI+DENI+UTNI+UTNIA)
hpernit_1.f:        CALL OMSTR (NITRF)
hpernit_1.f:      TAMSU= AMSU+ DESAM+ AMMIF- (ADSAM+IMMAM+UTAM+UTAMA+NITRF+AMVO)
hpernit_1.f:        CALL OMSTR (NITRF)
hpernit_1.f:        NITRF= NITRF*FRAC
hpernit_1.f:        NO3  = NO3+ NITRF- (IMMNI+DENI+UTNI+UTNIA)
hpernit_1.f:        AMSU = AMSU+ DESAM+ AMMIF- (ADSAM+IMMAM+UTAM+UTAMA+NITRF+AMVO)
hpernit_1.f:      NITRXF(1)= ADSAM
hpernit_1.f:      NITRXF(2)= DESAM
hpernit_1.f:      NITRXF(3)= AMMIF
hpernit_1.f:      NITRXF(4)= IMMAM
hpernit_1.f:      NITRXF(5)= UTAM
hpernit_1.f:      NITRXF(6)= IMMNI
hpernit_1.f:      NITRXF(7)= UTNI
hpernit_1.f:      NITRXF(8)= NITRF
hpernit_1.f:      NITRXF(9)= DENI
hpernit_1.f:      NITRXF(10)= AMVO
hpernit_1.f:      NITRXF(11)= RFON
hpernit_1.f:      NITRXF(12)= PRETL
hpernit_1.f:      NITRXF(13)= PRETR
hpernit_1.f:      NITRXF(14)= UTAMA
hpernit_1.f:      NITRXF(15)= UTNIA
hpernit_1.f:      NITRXF(16)= NFIX
