hpernit_nut22d.f:C     + + + COMMON BLOCKS- SCRTCH, VERSION NITR2 + + +
hpernit_nut22d.f:      SUBROUTINE   NITRXN
hpernit_nut22d.f:     M                    NDEFC,NWCNT,NECNT,AGPLTN,NIT,NITRXF  ! )
hpernit_nut22d.f:     $            KRET,KRETF,ANUTF,SMST,NDEFC,AGPLTN,NIT(8),NITRXF(16)
hpernit_nut22d.f:C     NITRXF - current reaction fluxes for this layer
hpernit_nut22d.f:     $            AMMIF,IMMAM,UTAM,IMMNI,UTNI,NITRF,DENI,AMVO,RFON,
hpernit_nut22d.f:      ADSAM= NITRXF(1)
hpernit_nut22d.f:      DESAM= NITRXF(2)
hpernit_nut22d.f:      AMMIF= NITRXF(3)
hpernit_nut22d.f:      IMMAM= NITRXF(4)
hpernit_nut22d.f:      UTAM = NITRXF(5)
hpernit_nut22d.f:      IMMNI= NITRXF(6)
hpernit_nut22d.f:      UTNI = NITRXF(7)
hpernit_nut22d.f:      NITRF= NITRXF(8)
hpernit_nut22d.f:      DENI = NITRXF(9)
hpernit_nut22d.f:      AMVO = NITRXF(10)
hpernit_nut22d.f:      RFON = NITRXF(11)
hpernit_nut22d.f:      PRETL= NITRXF(12)
hpernit_nut22d.f:      PRETR= NITRXF(13)
hpernit_nut22d.f:      UTAMA= NITRXF(14)
hpernit_nut22d.f:      UTNIA= NITRXF(15)
hpernit_nut22d.f:      NFIX = NITRXF(16)
hpernit_nut22d.f:          NITRF= AMSU*KNIK
hpernit_nut22d.f:          NITRF= 0.0
hpernit_nut22d.f:      TNO3= NO3+ NITRF- (IMMNI+DENI+UTNI+UTNIA)
hpernit_nut22d.f:        CALL OMSTR (NITRF)
hpernit_nut22d.f:      TAMSU= AMSU+ DESAM+ AMMIF- (ADSAM+IMMAM+UTAM+UTAMA+NITRF+AMVO)
hpernit_nut22d.f:        CALL OMSTR (NITRF)
hpernit_nut22d.f:        NITRF= NITRF*FRAC
hpernit_nut22d.f:        NO3  = NO3+ NITRF- (IMMNI+DENI+UTNI+UTNIA)
hpernit_nut22d.f:        AMSU = AMSU+ DESAM+ AMMIF- (ADSAM+IMMAM+UTAM+UTAMA+NITRF+AMVO)
hpernit_nut22d.f:      NITRXF(1)= ADSAM
hpernit_nut22d.f:      NITRXF(2)= DESAM
hpernit_nut22d.f:      NITRXF(3)= AMMIF
hpernit_nut22d.f:      NITRXF(4)= IMMAM
hpernit_nut22d.f:      NITRXF(5)= UTAM
hpernit_nut22d.f:      NITRXF(6)= IMMNI
hpernit_nut22d.f:      NITRXF(7)= UTNI
hpernit_nut22d.f:      NITRXF(8)= NITRF
hpernit_nut22d.f:      NITRXF(9)= DENI
hpernit_nut22d.f:      NITRXF(10)= AMVO
hpernit_nut22d.f:      NITRXF(11)= RFON
hpernit_nut22d.f:      NITRXF(12)= PRETL
hpernit_nut22d.f:      NITRXF(13)= PRETR
hpernit_nut22d.f:      NITRXF(14)= UTAMA
hpernit_nut22d.f:      NITRXF(15)= UTNIA
hpernit_nut22d.f:      NITRXF(16)= NFIX
