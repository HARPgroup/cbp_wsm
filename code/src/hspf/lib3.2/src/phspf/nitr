hper.f:     $          PMSTLA,PPEST,PNITR,PPHOS,PTRACR,PERRST,PUTOSV
hper.f:        IF (NITRFG.EQ.1) THEN
hper.f:          CALL PNITR
hper.f:     $            PSTEMP, PWTGAS, PQUAL, MSTLAY, PEST, NITR, PHOS,
hper.f:          IF (NITRFG .EQ. 1) THEN
hper.f:            CALL NITR
hper.f:      EXTERNAL SNOWPB,PWATPB,SEDTPB,PWGSPB,PQALPB,PESTPB,NITRPB,
hper.f:        IF (NITRFG .EQ. 1) THEN
hper.f:          CALL NITRPB
hper.f:        IF (NITRFG .EQ. 1 .AND. PFLAG(10) .LT. 6) THEN
hper.f:        IF (NITRFG .EQ. 1 .AND. PFLAG(10) .LE. LEV) THEN
hper.f:      EXTERNAL   SNORST,PWARST,SDRST,PQRST,PSTRST,NITRST,
hper.f:        IF (NITRFG .EQ. 1 .AND. PFLAG(10) .LT. 6) THEN
hper.f:          CALL NITRST
hper.f:     $         MSTLPT,PESTPT,NITRPT,PHOSPT,TRACPT
hper.f:        IF (NITRFG .EQ. 1) THEN
hper.f:          CALL NITRPT
hpernit_1.f:      SUBROUTINE   PNITR
