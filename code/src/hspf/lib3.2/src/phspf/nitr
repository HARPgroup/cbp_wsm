hpernit_nut22i.f:      SUBROUTINE   NITRPT
hpernit_nut22i.f:C     + + + COMMON BLOCKS- SCRTCH, VERSION NITR2 + + +
hpernit_nut22i.f:      SUBROUTINE   NITRST
hpernit_nut22i.f:C     + + + COMMON BLOCKS- SCRTCH, VERSION NITR2 + + +
hpernit_nut22i.f:      SUBROUTINE   NITRXN
hpernit_nut22i.f:     M                    NDEFC,NWCNT,NECNT,AGPLTN,NIT,NITRXF  ! )
hpernit_nut22i.f:     $            KRET,KRETF,ANUTF,SMST,NDEFC,AGPLTN,NIT(8),NITRXF(16)
hpernit_nut22i.f:C     NITRXF - current reaction fluxes for this layer
hpernit_nut22i.f:     $            AMMIF,IMMAM,UTAM,IMMNI,UTNI,NITRF,DENI,AMVO,RFON,
hpernit_nut22i.f:      ADSAM= NITRXF(1)
hpernit_nut22i.f:      DESAM= NITRXF(2)
hpernit_nut22i.f:      AMMIF= NITRXF(3)
hpernit_nut22i.f:      IMMAM= NITRXF(4)
hpernit_nut22i.f:      UTAM = NITRXF(5)
hpernit_nut22i.f:      IMMNI= NITRXF(6)
hpernit_nut22i.f:      UTNI = NITRXF(7)
hpernit_nut22i.f:      NITRF= NITRXF(8)
hpernit_nut22i.f:      DENI = NITRXF(9)
hpernit_nut22i.f:      AMVO = NITRXF(10)
hpernit_nut22i.f:      RFON = NITRXF(11)
hpernit_nut22i.f:      PRETL= NITRXF(12)
hpernit_nut22i.f:      PRETR= NITRXF(13)
hpernit_nut22i.f:      UTAMA= NITRXF(14)
hpernit_nut22i.f:      UTNIA= NITRXF(15)
hpernit_nut22i.f:      NFIX = NITRXF(16)
hpernit_nut22i.f:          NITRF= AMSU*KNIK
hpernit_nut22i.f:          NITRF= 0.0
hpernit_nut22i.f:      TNO3= NO3+ NITRF- (IMMNI+DENI+UTNI+UTNIA)
hpernit_nut22i.f:        CALL OMSTR (NITRF)
hpernit_nut22i.f:      TAMSU= AMSU+ DESAM+ AMMIF- (ADSAM+IMMAM+UTAM+UTAMA+NITRF+AMVO)
hpernit_nut22i.f:        CALL OMSTR (NITRF)
hpernit_nut22i.f:        NITRF= NITRF*FRAC
hpernit_nut22i.f:        NO3  = NO3+ NITRF- (IMMNI+DENI+UTNI+UTNIA)
hpernit_nut22i.f:        AMSU = AMSU+ DESAM+ AMMIF- (ADSAM+IMMAM+UTAM+UTAMA+NITRF+AMVO)
hpernit_nut22i.f:      NITRXF(1)= ADSAM
hpernit_nut22i.f:      NITRXF(2)= DESAM
hpernit_nut22i.f:      NITRXF(3)= AMMIF
hpernit_nut22i.f:      NITRXF(4)= IMMAM
hpernit_nut22i.f:      NITRXF(5)= UTAM
hpernit_nut22i.f:      NITRXF(6)= IMMNI
hpernit_nut22i.f:      NITRXF(7)= UTNI
hpernit_nut22i.f:      NITRXF(8)= NITRF
hpernit_nut22i.f:      NITRXF(9)= DENI
hpernit_nut22i.f:      NITRXF(10)= AMVO
hpernit_nut22i.f:      NITRXF(11)= RFON
hpernit_nut22i.f:      NITRXF(12)= PRETL
hpernit_nut22i.f:      NITRXF(13)= PRETR
