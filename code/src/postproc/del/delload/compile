#/bin/csh
set FC = $argv[1]
set usrdir = ../../../lib
set usrlib = ($usrdir/ttyux.o $usrdir/get_lib.a $usrdir/util_lib.a)

$FC -fbounds-check -c main.f
$FC -fbounds-check -c delatdepann.f
$FC -fbounds-check -c delatdepave.f
$FC -fbounds-check -c delatdepmon.f
$FC -fbounds-check -c deldatann.f
$FC -fbounds-check -c deldatave.f
$FC -fbounds-check -c deldatmon.f
$FC -fbounds-check -c delinfo.f
$FC -fbounds-check -c dellsegann.f
$FC -fbounds-check -c dellsegave.f
$FC -fbounds-check -c dellsegmon.f
$FC -fbounds-check -c getdfs.f
$FC -fbounds-check -c getloads.f
$FC -fbounds-check -c getRvars.f
$FC -fbounds-check -c readcontrol_dat.f

$FC -o ../../../../bin/DEL.exe main.o delatdepann.o delatdepave.o delatdepmon.o deldatann.o deldatave.o deldatmon.o delinfo.o dellsegann.o dellsegave.o dellsegmon.o getdfs.o getloads.o getRvars.o readcontrol_dat.o $usrlib

rm *.o

