  V # -  #  NH4-sand  NH4-silt  NH4-clay  PO4-sand  PO4-silt  PO4-clay ***
END NUTRX
PLANK
  PLNK-FLAGS       8i5
  V RCHRES PHYF ZOOF BALF SDLT AMRF DECF NSFG ZFOO ***
  PLK-AD-FLAGS      unique
    RCHRES   Atmospheric Deposition Flags ***
    <PLS >   ORN    ORP    ORC    ***
  V # -  # ONFONC OPFOPC OCFOCC   ***
  PLNK-PARM1       6f10
  V RCHRES    RATCLP    NONREF    LITSED     ALNPR      EXTB     MALGR ***
  PLNK-PARM2       7f10
  V ***RES     CMMLT      CMMN     CMMNP      CMMP    TALGRH    TALGRL    TALGRM
    # -  # ***ly/min      mg/l      mg/l      mg/l     deg F     deg F     deg F
  PLNK-PARM3       6f10
  V RCHRES     ALR20      ALDH      ALDL     OXALD     NALDH     PALDH ***
    # -  #       /hr       /hr       /hr       /hr      mg/l      mg/l ***
  PHYTO-PARM       6f10
  V RCHRES      SEED    MXSTAY      OREF    CLALDH    PHYSET    REFSET ***
    # -  #      mg/l      mg/l                ug/l                     ***
  BENAL-PARM       3f10
  V RCHRES      MBAL    CFBALR    CFBALG*** 
  PLNK-INIT        6f10
  V RCHRES     PHYTO       ZOO     BENAL       ORN       ORP       ORC ***
    # -  #      mg/l     org/l     mg/m2      mg/l      mg/l      mg/l ***
END PLANK
