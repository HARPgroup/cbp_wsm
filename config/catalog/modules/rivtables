  HEAT-INIT        2f10
  V # -  #        TW    AIRTMP ***
END HTRCH
SEDTRN
  SANDFG           1i5
  V # -  #SNDFG ***
  SED-GENPARM      3f10
  V RCHRES    BEDWID    BEDWRN       POR ***
    # -  #      (ft)      (ft)       (-) ***
  SAND-PM          5f10
  V RCHRES         D         W       RHO     KSAND    EXPSND ***
    # -  #      (in)    (in/s)   (g/cm3)                     ***
  SILT-CLAY-PM#1   6f10
  V RCHRES        SD        SW      SRHO    STAUCD    STAUCS        SM ***
    # -  #      (in)    (in/s)   (g/cm3)                               ***
  SILT-CLAY-PM#2   6f10
  V RCHRES        CD        CW      CRHO    CTAUCD    CTAUCS        CM ***
    # -  #      (in)    (in/s)   (g/cm3)                               ***
  SSED-INIT        3f10
    RCHRES     Suspended sed concs (mg/l) ***
  V # -  #      SAND      SILT      CLAY  ***
  BED-INIT         4f10
    RCHRES      (ft)  Initial bed composition      ***
  V # -  #    BEDDEP      SAND      SILT      CLAY ***
END SEDTRN
