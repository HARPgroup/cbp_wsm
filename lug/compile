#!/bin/csh
set FC = $argv[1]
source ../../set_wdmlibdir
set wdmlibdir = ../$wdmlibdir
set lib1 = $wdmlibdir/wdmlib.a
set lib2 = $wdmlibdir/adwdmlib.a
set lib3 = $wdmlibdir/utillib.a
set dsnlib = ../lib/dsn/dsn_utils.o

set usrdir = ../lib
set usrlib = ($usrdir/ttyux.o $usrdir/get_lib.a $usrdir/util_lib.a)

$FC -fbounds-check -c actions.f
$FC -fbounds-check -c extsources.f
$FC -fbounds-check -c exttargets.f
$FC -fbounds-check -c files.f
$FC -fbounds-check -c getScenarioMod.f 
$FC -fbounds-check -c getSpecies2Qual.f
$FC -fbounds-check -c getSpecSpecies.f
$FC -fbounds-check -c getspecflags.f
$FC -fbounds-check -c global.f
$FC -fbounds-check -c lspecact.f
$FC -fbounds-check -c main.f
$FC -fbounds-check -c network.f
$FC -fbounds-check -c opseq.f
$FC -fbounds-check -c pgen.f
$FC -fbounds-check -c PQUALactions.f
$FC -fbounds-check -c readcontrol1key.f
$FC -fbounds-check -c readcontrol_Lmodules.f
$FC -fbounds-check -c readvalues.f
$FC -fbounds-check -c readrorgn.f
$FC -fbounds-check -c readtables.f
$FC -fbounds-check -c rmMissing.f
$FC -fbounds-check -c uvnames.f
$FC -fbounds-check -c uvquans.f
$FC -fbounds-check -c writeapp.f
$FC -fbounds-check -c writeatdep.f
$FC -fbounds-check -c writeSpecActQual.f
$FC -fbounds-check -c writetables.f

$FC -fbounds-check -c main_writeapp_only.f
$FC -fbounds-check -c lspecwrite.f
$FC -fbounds-check -c actionwrite.f

$FC -o ../../bin/lug.exe actions.o extsources.o exttargets.o files.o getScenarioMod.o getSpecies2Qual.o getSpecSpecies.o getspecflags.o global.o lspecact.o main.o network.o opseq.o pgen.o PQUALactions.o readcontrol1key.o readcontrol_Lmodules.o readvalues.o readtables.o rmMissing.o uvnames.o uvquans.o writeapp.o readrorgn.o writeatdep.o writeSpecActQual.o writetables.o $usrlib $dsnlib $lib1 $lib2 $lib3 
$FC -o ../../bin/write_actions_only.exe actionwrite.o extsources.o exttargets.o files.o getSpecies2Qual.o getScenarioMod.o getSpecSpecies.o getspecflags.o global.o lspecwrite.o main_writeapp_only.o network.o opseq.o pgen.o readcontrol1key.o readcontrol_Lmodules.o readvalues.o rmMissing.o uvnames.o uvquans.o writeapp.o writeatdep.o $usrlib $dsnlib $lib1 $lib2 $lib3 

rm *.o
