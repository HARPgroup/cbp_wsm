#/bin/csh
set FC = $argv[1]
set usrdir = ../../../lib
set usrlib = ($usrdir/ttyux.o $usrdir/get_lib.a $usrdir/util_lib.a)

$FC -fbounds-check -c main.f
$FC -fbounds-check -c main_otherDF.f
$FC -fbounds-check -c getDFmethod.f
$FC -fbounds-check -c getloads.f
$FC -fbounds-check -c getRvars.f
$FC -fbounds-check -c delinfo.f

$FC -o ../../../../bin/sum_delivery_factor.exe main.o getDFmethod.o getloads.o getRvars.o delinfo.o $usrlib
$FC -o ../../../../bin/sum_otherDF_delivery_factor.exe main_otherDF.o getDFmethod.o getloads.o getRvars.o delinfo.o $usrlib

rm *.o

